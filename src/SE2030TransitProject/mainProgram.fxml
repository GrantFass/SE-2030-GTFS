<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox maxHeight="-Infinity" minHeight="-Infinity" maxWidth="-Infinity"
      minWidth="-Infinity" prefHeight="800.0" prefWidth="661.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      fx:controller="SE2030TransitProject.Controller"
      xmlns:fx="http://javafx.com/fxml">
    <MenuBar>
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#close" text="Close"/>
            <MenuItem mnemonicParsing="false" onAction="#save" text="Save"/>
            <MenuItem mnemonicParsing="false" onAction="#load" text="Load"/>
            <MenuItem mnemonicParsing="false" onAction="#undo" text="Undo"/>
            <MenuItem mnemonicParsing="false" onAction="#reload" text="Reload"/>
        </Menu>
        <Menu mnemonicParsing="false" text="View">
            <MenuItem mnemonicParsing="false" onAction="#toggleDataDisplay"
                      text="Show Data Display" fx:id="viewDataDisplayButton"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" onAction="#displayHelp" text="About"/>
        </Menu>
    </MenuBar>
    <Label fx:id="topLabel" alignment="CENTER" prefHeight="38.0" prefWidth="661.0" text="Information"
           textAlignment="CENTER">
        <font>
            <Font name="Courier New" size="14.0"/>
        </font>
    </Label>
    <TextArea fx:id="textArea" text="Information Goes Here" editable="false" prefHeight="700"/>
</VBox>
